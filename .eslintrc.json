{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn"
  },
  "ignorePatterns": ["coverage/"]
}
