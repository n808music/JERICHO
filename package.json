{
  "name": "jericho-system",
  "version": "0.1.0",
  "description": "Jericho closed-loop behavioral execution MVP",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "node src/api/server.js",
    "dev:client": "vite dev",
    "build": "npm run lint && npm test && vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint src tests"
  },
  "keywords": [
    "jericho",
    "identity",
    "tasks",
    "reinforcement"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "vite": "^5.4.10"
  }
}
